{
  "name": "vite-template",
  "version": "0.0.0",
  "license": "ISC",
  "scripts": {
    "dev": "vite --host",
    "build": "vue-tsc --noEmit && vite build",
    "serve": "vite preview",
    "lint:eslint": "eslint src --fix --ext .ts,.tsx,.vue ",
    "lint:prettier": "prettier --write  \"src/**/*.{js,json,ts,tsx,css,less,scss,vue,html,md}\"",
    "lint:stylelint": "stylelint src/styles/**/*.scss src/pages/**/*.scss --fix",
    "lint:lint-staged": "lint-staged",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.md": "prettier --write",
    "*.{ts,tsx,js,vue,scss}": "prettier --write",
    "*.{ts,tsx,js,vue}": "eslint --fix"
  },
  "dependencies": {
    "axios": "^0.24.0",
    "lib-flexible": "^0.3.2",
    "postcss-pxtorem": "^6.0.0",
    "prettier": "^2.4.1",
    "qs": "^6.10.1",
    "vant": "^3.2.7",
    "vconsole": "^3.9.4",
    "vue": "^3.2.16",
    "vue-router": "^4"
  },
  "devDependencies": {
    "@types/node": "^16.11.6",
    "@types/qs": "^6.9.7",
    "@typescript-eslint/parser": "^5.3.0",
    "@typescript-eslint/types": "^5.3.0",
    "@vitejs/plugin-legacy": "^1.6.2",
    "@vitejs/plugin-vue": "^1.9.3",
    "eslint": "^8.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.0.3",
    "husky": "^7.0.0",
    "lint-staged": "^11.2.6",
    "sass": "^1.43.4",
    "stylelint": "^14.0.1",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-standard": "^23.0.0",
    "stylelint-order": "^5.0.0",
    "stylelint-scss": "^4.0.0",
    "typescript": "^4.4.3",
    "vite": "^2.6.4",
    "vite-plugin-compression": "^0.3.5",
    "vite-plugin-style-import": "^1.3.0",
    "vite-plugin-vconsole": "^1.1.0",
    "vue-tsc": "^0.3.0"
  }
}
